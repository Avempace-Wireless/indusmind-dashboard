{
  "projectName": "Stitch Dashboard - Energy Monitoring System",
  "description": "Comprehensive component pattern analysis from 16 Stitch design screens",
  "extractionDate": "2026-01-06",
  "totalScreens": 16,
  "framework": "Vue 3 + Tailwind CSS + TypeScript",
  "designLanguage": "Modern Energy Monitoring Dashboard",
  "colorPalette": {
    "primary": {
      "hex": "#135bec",
      "rgb": "rgb(19, 91, 236)",
      "name": "Electric Blue",
      "usage": "Primary actions, active states, main brand color"
    },
    "primaryDark": {
