    },
    "darkCardActive": {
      "hex": "#232f48",
      "rgb": "rgb(35, 47, 72)",
      "name": "Dark Card Active",
      "usage": "Hover states on dark backgrounds"
    },
    "sidebarDark": {
      "hex": "#111722",
      "rgb": "rgb(17, 23, 34)",
      "name": "Sidebar Dark",
      "usage": "Sidebar background"
    },
    "borderDark": {
      "hex": "#324467",
      "rgb": "rgb(50, 68, 103)",
      "name": "Dark Border",
      "usage": "Borders in dark mode"
    },
    "textSecondary": {
      "hex": "#92a4c9",
      "rgb": "rgb(146, 164, 201)",
      "name": "Secondary Text",
      "usage": "Secondary text, labels, descriptions"
    },
    "slate": {
      "50": "#f8fafc",
      "100": "#f1f5f9",
      "200": "#e2e8f0",
      "300": "#cbd5e1",
      "400": "#94a3b8",
      "500": "#64748b",
      "600": "#475569",
      "700": "#334155",
      "800": "#1e293b",
      "900": "#0f172a"
    }
  },
  "typography": {
    "fontFamily": {
      "primary": "Inter",
      "fallback": "sans-serif"
    },
    "headings": {
      "h1": {
        "fontSize": "2.25rem",
        "lineHeight": "2.5rem",
        "fontWeight": "900",
        "letterSpacing": "-0.02em",
        "usage": "Page titles"
      },
      "h2": {
        "fontSize": "2rem",
        "lineHeight": "2.5rem",
        "fontWeight": "800",
        "letterSpacing": "-0.01em",
        "usage": "Section headers"
      },
      "h3": {
        "fontSize": "1.5rem",
        "lineHeight": "2rem",
        "fontWeight": "700",
        "usage": "Subsection headers"
      },
      "h4": {
        "fontSize": "1.125rem",
        "lineHeight": "1.75rem",
        "fontWeight": "700",
        "usage": "Widget titles"
      }
    },
    "body": {
      "base": {
        "fontSize": "1rem",
        "lineHeight": "1.5rem",
        "fontWeight": "400",
        "usage": "Body text, paragraphs"
      },
      "medium": {
        "fontSize": "1rem",
        "lineHeight": "1.5rem",
        "fontWeight": "500",
        "usage": "Emphasized text, labels"
      },
      "bold": {
        "fontSize": "1rem",
        "lineHeight": "1.5rem",
        "fontWeight": "700",
        "usage": "Strong text, KPI values"
      }
    },
    "small": {
      "fontSize": "0.875rem",
      "lineHeight": "1.25rem",
      "fontWeight": "500",
      "usage": "Secondary labels, helper text"
    },
    "tiny": {
      "fontSize": "0.75rem",
      "lineHeight": "1rem",
      "fontWeight": "500",
      "usage": "Badges, small labels, timestamps"
    }
  },
  "spacing": {
    "gaps": {
      "xs": "0.5rem",
      "sm": "1rem",
      "md": "1.5rem",
      "lg": "2rem",
      "xl": "2.5rem",
      "2xl": "3rem"
    },
    "padding": {
      "card": "1.5rem",
      "section": "2rem",
      "container": "2.5rem"
    },
    "margins": {
      "section": "2rem",
      "element": "1rem"
    }
  },
  "borderRadius": {
    "none": "0",
    "xs": "0.125rem",
    "sm": "0.25rem",
    "base": "0.25rem",
    "md": "0.5rem",
    "lg": "0.5rem",
    "xl": "0.75rem",
    "full": "9999px"
  },
  "componentPatterns": {
    "cards": {
      "statCard": {
        "description": "KPI display card for metrics like power, voltage, frequency",
        "structure": {
          "layout": "flex flex-col gap-1",
          "padding": "1.25rem",
          "border": "1px solid (light: #e5e7eb, dark: #324467)",
          "borderRadius": "0.75rem",
          "shadow": "shadow-sm",
          "background": "light: #ffffff, dark: #1e293b"
        },
        "components": {
          "header": {
            "layout": "flex items-center justify-between",
            "label": "text-sm font-medium text-secondary",
            "icon": "material-symbols-outlined (24px)"
          },
          "value": {
            "layout": "flex items-baseline gap-2",
            "main": "text-2xl font-bold text-slate-900 dark:text-white",
            "unit": "text-lg text-slate-400 font-normal",
            "trend": "flex items-center text-xs font-medium (green/red)"
          },
          "progressBar": {
            "optional": true,
            "height": "0.25rem",
            "borderRadius": "0.5rem",
            "background": "light: #e5e7eb, dark: #324467",
            "fill": "gradient-to-r (primary to cyan-400)"
          }
        },
        "interactions": {
          "hover": "shadow-md transition-all",
          "group": "group hover:shadow-md"
        }
      },
      "chartCard": {
        "description": "Container for data visualizations with header and controls",
        "structure": {
          "layout": "flex flex-col rounded-xl",
          "padding": "1.5rem",
          "border": "1px solid",
          "background": "light: #ffffff, dark: #1e293b",
          "minHeight": "400px"
        },
        "components": {
          "header": {
            "layout": "flex items-start justify-between",
            "title": "text-lg font-bold text-slate-900 dark:text-white",
            "subtitle": "text-sm text-text-secondary",
            "controls": "flex items-center gap-2"
          },
          "chart": {
            "type": "SVG or canvas-based visualization",
            "gridLines": "dashed borders for reference",
            "gradient": "linear gradient fill beneath curves"
          },
          "legend": {
            "layout": "flex items-center gap-2",
            "items": "flex items-center gap-2"
          }
        }
      },
      "alertCard": {
        "description": "Alert/notification display card",
        "structure": {
          "layout": "flex items-center gap-4",
          "padding": "1rem",
          "borderLeft": "4px solid (red/orange/green/blue)"
        },
        "components": {
          "icon": {
            "size": "40px",
            "background": "bg-color/10 (colored by severity)",
            "borderRadius": "rounded-full",
            "icon": "warning, info, check_circle, etc."
          },
          "content": {
            "layout": "flex flex-1 flex-col",
            "title": "text-sm font-semibold text-slate-900 dark:text-white",
            "description": "text-xs text-text-secondary"
          },
          "badge": {
            "type": "inline-flex items-center",
            "style": "rounded-full border px-2 py-1",
            "text": "text-xs font-medium uppercase"
          }
        }
      }
    },
    "buttons": {
      "primaryButton": {
        "description": "Main action button (blue)",
        "padding": "px-4 py-2",
        "height": "h-10",
        "background": "bg-primary hover:bg-primary-dark",
        "text": "text-white text-sm font-bold",
        "shadow": "shadow-lg shadow-primary/20",
        "borderRadius": "rounded-lg",
        "transition": "transition-all transform active:scale-95"
      },
      "secondaryButton": {
        "description": "Secondary action button",
        "padding": "px-4 py-2",
        "height": "h-10",
        "border": "border border-slate-200 dark:border-slate-700",
        "background": "bg-white dark:bg-surface-dark",
        "text": "text-slate-700 dark:text-slate-300",
        "hover": "hover:bg-slate-50 dark:hover:bg-slate-700",
        "transition": "transition-colors"
      },
      "iconButton": {
        "description": "Button with icon only",
        "size": "h-10 w-10",
        "display": "flex items-center justify-center",
        "borderRadius": "rounded-lg",
        "hover": "hover:bg-slate-100 dark:hover:bg-slate-800"
      },
      "ghostButton": {
        "description": "Transparent button",
        "background": "bg-transparent",
        "text": "text-primary hover:text-primary-dark",
        "hover": "hover:underline"
      }
    },
    "inputs": {
      "textInput": {
        "height": "h-11",
        "padding": "px-4 py-2.5",
        "border": "border border-slate-200 dark:border-slate-700",
        "borderRadius": "rounded-lg",
        "background": "bg-slate-50 dark:bg-slate-800",
        "text": "text-slate-900 dark:text-white text-sm",
        "placeholder": "placeholder:text-slate-400 dark:placeholder:text-slate-500",
        "focus": "focus:ring-2 focus:ring-primary focus:border-transparent focus:outline-none"
      },
      "selectInput": {
        "height": "h-11",
        "padding": "pl-3 pr-10",
        "appearance": "appearance-none",
        "background": "bg-slate-50 dark:bg-slate-800",
        "icon": "absolute right-3 top-1/2 -translate-y-1/2",
        "chevron": "material-symbols-outlined expand_more"
      },
      "rangeSlider": {
        "appearance": "appearance-none",
        "height": "h-2",
        "background": "bg-slate-200 dark:bg-slate-700",
        "borderRadius": "rounded-lg",
        "thumb": {
          "appearance": "-webkit-appearance: none",
          "background": "bg-primary",
          "size": "h-5 w-5",
          "borderRadius": "rounded-full",
          "shadow": "shadow-sm"
        }
      },
      "checkbox": {
        "size": "w-4 h-4",
        "borderRadius": "rounded border-slate-300",
        "background": "bg-transparent",
        "accent": "accent-primary",
        "checked": "peer-checked:bg-primary"
      },
      "toggleSwitch": {
        "width": "w-11",
        "height": "h-6",
        "borderRadius": "rounded-full",
        "background": "bg-slate-200 dark:bg-slate-700",
        "checked": "peer-checked:bg-primary",
        "thumb": {
          "size": "h-5 w-5",
          "position": "peer-checked:translate-x-full",
          "transition": "transition-all"
        }
      }
    },
    "tables": {
      "structure": {
        "container": "overflow-x-auto rounded-xl border shadow-sm",
        "header": {
          "background": "bg-slate-50 dark:bg-slate-800",
          "border": "border-b border-slate-200 dark:border-slate-700",
          "text": "text-xs uppercase tracking-wider text-slate-500 dark:text-slate-400"
        },
        "body": {
          "border": "divide-y divide-slate-200 dark:divide-slate-700",
          "rowHover": "hover:bg-slate-50 dark:hover:bg-slate-800/50"
        }
      },
      "cells": {
        "padding": "px-6 py-4",
        "text": "text-sm text-slate-700 dark:text-slate-300",
        "font": "font-medium"
      },
      "statusBadge": {
        "onlineGreen": "bg-green-100 dark:bg-green-500/10 text-green-700 dark:text-green-400",
        "maintenanceAmber": "bg-amber-100 dark:bg-amber-500/10 text-amber-700 dark:text-amber-400",
        "offlineGrey": "bg-slate-100 dark:bg-slate-700 text-slate-600 dark:text-slate-400",
        "errorRed": "bg-red-100 dark:bg-red-500/10 text-red-700 dark:text-red-400",
        "borderRadius": "rounded-full",
        "padding": "px-2.5 py-1",
        "fontSize": "text-xs",
        "fontWeight": "font-medium"
      }
    },
    "badges": {
      "alertBadge": {
        "critical": {
          "background": "bg-red-500/10 dark:bg-red-900/20",
          "border": "border border-red-200 dark:border-red-500/30",
          "text": "text-red-700 dark:text-red-400",
          "padding": "px-2 py-0.5",
          "fontSize": "text-xs",
          "fontWeight": "font-medium",
          "borderRadius": "rounded-full"
        },
        "warning": {
          "background": "bg-orange-100 dark:bg-orange-500/20",
          "border": "border border-orange-200 dark:border-orange-500/30",
          "text": "text-orange-700 dark:text-orange-400"
        },
        "info": {
          "background": "bg-blue-100 dark:bg-blue-500/20",
          "border": "border border-blue-200 dark:border-blue-500/30",
          "text": "text-blue-700 dark:text-blue-400"
        }
      },
      "statusBadge": {
        "online": "bg-emerald-500/10 text-emerald-500",
        "offline": "bg-slate-500/10 text-slate-500",
        "maintenance": "bg-amber-500/10 text-amber-500",
        "error": "bg-red-500/10 text-red-500"
      }
    },
    "progressBars": {
      "structure": {
        "container": "h-2 w-full bg-slate-100 dark:bg-slate-700 rounded-full overflow-hidden",
        "fill": "h-full rounded-full transition-all"
      },
      "colors": {
        "success": "bg-emerald-500",
        "warning": "bg-amber-500",
        "danger": "bg-red-500",
        "info": "bg-primary",
        "neutral": "bg-slate-400"
      },
      "labels": {
        "layout": "flex justify-between text-xs font-medium",
        "text": "text-slate-500 dark:text-slate-400",
        "value": "text-slate-900 dark:text-white"
      }
    },
    "modals": {
      "overlay": {
        "background": "bg-black/50 dark:bg-black/70",
        "position": "fixed inset-0"
      },
      "dialog": {
        "background": "bg-white dark:bg-surface-dark",
        "padding": "p-6",
        "borderRadius": "rounded-xl",
        "shadow": "shadow-xl",
        "width": "max-w-md md:max-w-lg",
        "maxHeight": "max-h-[90vh] overflow-y-auto"
      },
      "header": {
        "layout": "flex items-center justify-between",
        "title": "text-lg font-bold text-slate-900 dark:text-white",
        "closeButton": "text-slate-500 hover:text-slate-900"
      },
      "footer": {
        "layout": "flex items-center justify-end gap-3",
        "border": "border-t border-slate-200 dark:border-slate-700",
        "padding": "pt-6"
      }
    },
    "navigation": {
      "sidebar": {
        "width": "w-64",
        "background": "bg-white dark:bg-[#111722]",
        "border": "border-r border-slate-200 dark:border-slate-800",
        "padding": "p-4",
        "layout": "flex flex-col h-full justify-between"
      },
      "navItem": {
        "padding": "px-3 py-2.5",
        "borderRadius": "rounded-lg",
        "text": "text-sm font-medium",
        "default": "text-slate-600 dark:text-slate-400 hover:text-slate-900 dark:hover:text-white",
        "active": "bg-primary/10 dark:bg-primary/20 text-primary dark:text-white shadow-md",
        "transition": "transition-colors"
      },
      "topbar": {
        "height": "h-16",
        "background": "bg-background-light/80 dark:bg-background-dark/80",
        "border": "border-b border-slate-200 dark:border-slate-800",
        "backdrop": "backdrop-blur-md",
        "padding": "px-6 md:px-8",
        "layout": "flex items-center justify-between"
      }
    },
    "charts": {
      "lineChart": {
        "container": "relative w-full h-64 md:h-80",
        "svg": {
          "viewBox": "0 0 1000 200",
          "preserveAspectRatio": "none"
        },
        "gradient": {
          "fill": "url(#chartGradient)",
          "startColor": "#135bec",
          "startOpacity": "0.3",
          "endOpacity": "0"
        },
        "line": {
          "stroke": "#135bec",
          "strokeWidth": "2-3",
          "strokeLinecap": "round",
          "vectorEffect": "non-scaling-stroke"
        },
        "gridLines": "dashed borders at 25% intervals",
        "axisLabels": "text-xs text-slate-400 px-1"
      },
      "barChart": {
        "bars": {
          "background": "bg-primary or variant colors",
          "borderRadius": "rounded-t-sm",
          "hover": "group-hover:opacity-80",
          "transition": "transition-all"
        },
        "labels": {
          "position": "bottom",
          "text": "text-xs text-slate-400",
          "spacing": "justify-between"
        },
        "tooltip": {
          "position": "absolute -top-8 left-1/2 -translate-x-1/2",
          "background": "bg-slate-900 dark:bg-slate-800",
          "text": "text-white text-xs py-1 px-2",
          "opacity": "opacity-0 group-hover:opacity-100"
        }
      },
      "pieChart": {
        "legend": "flex items-center gap-2",
        "legendItem": "h-3 w-3 rounded-full",
        "percentageBar": {
          "container": "h-3 w-full bg-slate-100 dark:bg-slate-700 rounded-full overflow-hidden",
          "fill": "h-full rounded-full",
          "value": "text-sm font-bold text-slate-900 dark:text-white"
        }
      }
    },
    "dialogs": {
      "alertDialog": {
        "title": "text-lg font-bold text-slate-900 dark:text-white",
        "message": "text-sm text-slate-600 dark:text-slate-400",
        "buttons": "flex gap-3 mt-6"
      },
      "confirmDialog": {
        "layout": "flex flex-col gap-4",
        "icon": "h-12 w-12 rounded-full",
        "title": "text-lg font-bold",
        "description": "text-sm text-slate-600 dark:text-slate-400"
      }
    }
  },
  "layoutPatterns": {
    "mainLayout": {
      "structure": "flex h-screen w-full",
      "sidebar": {
        "width": "w-64 hidden md:flex",
        "position": "fixed or flex-shrink-0",
        "direction": "flex-col"
      },
      "mainContent": {
        "layout": "flex-1 flex flex-col h-full overflow-hidden",
        "header": "sticky top-0 z-20",
        "content": "flex-1 overflow-y-auto p-6 md:p-8"
      }
    },
    "gridLayouts": {
      "cardGrid": "grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-4 gap-4",
      "doubleColumn": "grid grid-cols-1 lg:grid-cols-2 gap-6",
      "tripleColumn": "grid grid-cols-1 md:grid-cols-3 gap-6",
      "complexLayout": "grid grid-cols-1 lg:grid-cols-3 gap-6 (xl:col-span-2 for main)"
    },
    "responsiveBreakpoints": {
      "mobile": "max-width: 640px (hidden sidebars, stacked layouts)",
      "tablet": "min-width: 768px (md: prefix, show sidebars)",
      "laptop": "min-width: 1024px (lg: prefix, full layouts)",
      "desktop": "min-width: 1280px (xl: prefix, advanced grids)"
    }
  },
  "interactionPatterns": {
    "hoverStates": {
      "cards": "shadow-md transition-all",
      "buttons": "bg-opacity-80 hover:scale-105",
      "tableRows": "hover:bg-slate-50 dark:hover:bg-slate-800/50",
      "icons": "group-hover:text-primary transition-colors",
      "links": "hover:text-primary hover:underline"
    },
    "activeStates": {
      "navItem": "bg-primary text-white shadow-lg",
      "button": "scale-95 active:scale-90",
      "tab": "border-b-2 border-primary text-primary"
    },
    "disabledStates": {
      "opacity": "opacity-50",
      "cursor": "cursor-not-allowed",
      "pointerEvents": "pointer-events-none"
    },
    "loadingStates": {
      "spinner": "animate-spin",
      "pulse": "animate-pulse",
      "skeleton": "bg-slate-200 dark:bg-slate-700 rounded animate-pulse"
    },
    "transitions": {
      "color": "transition-colors duration-200",
      "all": "transition-all duration-300",
      "opacity": "transition-opacity duration-200",
      "transform": "transition-transform duration-300"
    }
  },
  "screens": {
